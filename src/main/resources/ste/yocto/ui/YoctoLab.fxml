                </Menu>
            </menus>
        </MenuBar>
      <ScrollPane>
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0">
               <children>
                      <GridPane fx:id="worldPane">
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                     <padding>
                        <Insets bottom="5.0" top="5.0" />
                     </padding>
                      </GridPane>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
        <HBox alignment="CENTER" prefHeight="26.0" prefWidth="211.0">
            <children>
                <Button fx:id="playButton" alignment="CENTER" mnemonicParsing="false" onAction="#play" prefHeight="26.0" prefWidth="5.0" text="▶" textAlignment="CENTER" />
            <Button fx:id="pauseButton" disable="true" mnemonicParsing="false" onAction="#pause" text="■" />
            </children>
        </HBox>
    </children>
